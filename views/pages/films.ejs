<% include ../partials/head %>
<% include ../partials/header %>

<main>
    <div class="jumbotron">
        <div class="text-center">
            <h1>Filmes</h1>
            <div class="row">
                <div class="col-sm-12">
                    <% films.forEach(function(film) { %>

                    <a class="btn btn-primary prepend-top" href="/tv?id=<%= film.id %>&type=film"><%= film.title %></a>

                    <% }); %>
                </div>
            </div>
        </div>
    </div>
</main>

<% include ../partials/footer %>

